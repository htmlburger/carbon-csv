{

    "name": "htmlburger/carbon-csv",
    "description": "Simple CSV file parser",

    "require": {
        "php": "^7.1 || ^8.0"
    },

    "autoload": {
        "psr-4": {
            "Carbon_CSV\\": "src/"
        }
    },

    "require-dev": {
        "illuminate/support": "^5.4",
        "symfony/var-dumper": "^3.3",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": false
        }
    }
}
